<?xml version="1.0" encoding="UTF-8"?>
<!--
  RACE specifics replacing values in WorldWind's sys config (config/worldwind.xml)
  -->

<WorldWindConfiguration version="1">

    <Property name="gov.nasa.worldwind.avkey.InputHandlerClassName"
              value="gov.nasa.race.ww.RaceAWTInputHandler"/>

    <Property name="gov.nasa.worldwind.avkey.ViewClassName"
              value="gov.nasa.race.ww.RaceWWView"/>

    <Property name="gov.nasa.worldwind.avkey.ViewInputHandlerClassName"
              value="gov.nasa.race.ww.RaceWWViewController"/>

    <Property name="gov.nasa.worldwind.avkey.DataFileStoreClassName"
              value="gov.nasa.race.ww.ConfigurableWriteCache"/>

    <Property name="gov.nasa.worldwind.avkey.URLReadTimeout"
              value="15000"/>

</WorldWindConfiguration>